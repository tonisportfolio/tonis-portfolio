$("input:checkbox").click(function() {
    var actualTime = "";
    $(this).parent().toggleClass("highlight");
});

var countChecked = function() {
  var n = $("input:checked.grupp1").length;
  $( ".counter1" ).text( n + (n === 1 ? " /" : " /") + " 3" );
};
countChecked();
$( "input[type=checkbox]" ).on( "click", countChecked);
var countChecked2 = function() {
  var n = $("input:checked.grupp2").length;
  $( ".counter2" ).text( n + (n === 1 ? " /" : " /") + " 3" );
};
countChecked2();
$( "input[type=checkbox]" ).on( "click", countChecked2);
var countChecked3 = function() {
  var n = $("input:checked.grupp3").length;
  $( ".counter3" ).text( n + (n === 1 ? " /" : " /") + " 3" );
};
countChecked3();
$( "input[type=checkbox]" ).on( "click", countChecked3);
var countChecked4 = function() {
  var n = $("input:checked.grupp4").length;
  $( ".counter4" ).text( n + (n === 1 ? " /" : " /") + " 3" );
};
countChecked4();
$( "input[type=checkbox]" ).on( "click", countChecked4);
</script>
	
